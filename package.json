{
  "name": "trongate",
  "displayName": "Trongate",
  "description": "Trongate vscode Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "publisher": "Jake Castelli | Simon Field",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakecastelli/trongate-vscode"
  },
  "icon": "assets/logo.png",
  "homepage": "https://trongate.io",
  "activationEvents": [
    "onCommand:trongate.newTrongate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trongate.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "trongate.newTrongate",
        "title": "New Trongate Module"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "trongate.newTrongate",
          "group": "trongateGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "php",
        "path": "./snippets/bloc.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.49.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "eslint": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.161",
    "@types/mkdirp": "^1.0.1"
  }
}
